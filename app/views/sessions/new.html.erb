<%= form_with(scope: :session , url: login_path) do |form| %>
    <div class="field">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div class="actions">
    <%= form.submit "LOG IN"%>
  </div>
<% end %>